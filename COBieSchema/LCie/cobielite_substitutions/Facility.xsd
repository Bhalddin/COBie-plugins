<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:fac="http://facility.lcie.cobielite.cobie.erdc.org" xmlns:base="http://core.cobielite.cobie.erdc.org" xmlns:camed="http://jcam.org.uk/editor" targetNamespace="http://facility.lcie.cobielite.cobie.erdc.org" elementFormDefault="qualified" attributeFormDefault="qualified" version="RC2">
	<xs:import namespace="http://core.cobielite.cobie.erdc.org" schemaLocation="../../COBieLite/xsd/cobielite_core.xsd"/>
	<xs:annotation>
		<xs:documentation>
					A COBieLite Facilitywith cardinalities as prescribed by the responsibility matrix.
			</xs:documentation>
	</xs:annotation>
	

<xs:complexType name="FacilityType">
	<xs:annotation>
		<xs:documentation>A type for the Facility element that is the root of a COBie document</xs:documentation>
	</xs:annotation>
	<xs:complexContent>
		<xs:extension base="base:FacilityType">
			<xs:sequence>
				<xs:element ref="base:FacilityCategory"/>
				<xs:element ref="base:ProjectAssignment" />
				<xs:element ref="base:SiteAssignment" />
				<xs:element ref="base:FacilityDefaultLinearUnit" />
				<xs:element ref="base:FacilityDefaultAreaUnit" />
				<xs:element ref="base:FacilityDefaultVolumeUnit" />
				<xs:element ref="base:FacilityDefaultCurrencyUnit" />
				<xs:element ref="base:FacilityDefaultMeasurementStandard" />
				<xs:element ref="base:FacilityDescription" minOccurs="0" />
				<xs:element ref="base:FacilityDeliverablePhaseName" minOccurs="0"/>
				<xs:element ref="base:Floors" />
				<xs:element ref="base:Zones" minOccurs="0" />
				<xs:element ref="base:AssetTypes" minOccurs="0" />
				<xs:element ref="base:Systems" minOccurs="0" />
				<xs:element ref="base:Connections" minOccurs="0" />
				<xs:element ref="base:Contacts"/>
				<xs:element ref="base:FacilityAttributes" minOccurs="0" />
				<xs:element ref="base:FacilityDocuments" minOccurs="0" />
				<xs:element ref="base:FacilityIssues" minOccurs="0" />
			</xs:sequence>
		</xs:extension>
	</xs:complexContent>
</xs:complexType>
<xs:element name="Facility" type="fac:FacilityType" substitutionGroup="base:Facility">
	<xs:annotation>
		<xs:documentation>
			Facility contains information about the facility being exchanged during a given COBie deliverable.  Since COBie deliverables may be exchanged during planning, design, construction, handover, or operation phases there may be differences in facility designations.  COBie files will typically contain a single facility.
			</xs:documentation>
	</xs:annotation>
</xs:element>
</xs:schema>
