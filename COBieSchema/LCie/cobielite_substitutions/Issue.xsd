	</xs:annotation>
	<xs:complexContent>
		<xs:extension base="base:IssueType">
			<xs:sequence>
				<xs:element ref="base:IssueRiskText" />
				<xs:element ref="base:IssueSeverityText" />
				<xs:element ref="base:IssueImpactText" />
