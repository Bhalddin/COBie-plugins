		</xs:extension>
	</xs:complexContent>
</xs:complexType>
<xs:element name="Issue" type="issue:IssueType" substitutionGroup="base:Issue" id="d2e1495">
	<xs:annotation>
		<xs:documentation>A specific Issue within this set of Issues. </xs:documentation>
	</xs:annotation>
</xs:element>

</xs:schema>
