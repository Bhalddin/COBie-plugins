			<xs:selector xpath="base:AssetTypes/cobie:AssetType/base:Assets/cobie:Asset/base:AssetSpaceAssignments/base:SpaceAssignment"/>
			<xs:field xpath="cobie:SpaceName"/>
		</xs:keyref>
	</xs:element>
