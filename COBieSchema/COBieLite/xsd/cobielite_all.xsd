			<xs:field xpath="cobie:ZoneName"/>
			<xs:field xpath="cobie:ZoneCategory"/>
		</xs:keyref>
		<!--Space Key Refs-->
		<xs:keyref name="AssetSpaceKeyRef" refer="cobie:SpaceKey">
