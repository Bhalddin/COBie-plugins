	<xs:import namespace="http://issue.cobielite.cobie.erdc.org" schemaLocation="issue.xsd"/>
	<xs:annotation>
		<xs:documentation>
					A COBieLite Attribute with cardinalities as prescribed by the responsibility matrix.
			</xs:documentation>
